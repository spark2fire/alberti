{
  "properties": [
    {
      "name": "cn.spark2fire.auth.idle-timeout",
      "type": "java.lang.String",
      "description": "The token idle timeout (minutes).",
      "defaultValue": 30
    },
    {
      "name": "cn.spark2fire.auth.header-name",
      "type": "java.lang.String",
      "description": "The token header name.",
      "defaultValue": "X-ACCESS-TOKEN"
    },
    {
      "name": "cn.spark2fire.auth.table-name",
      "type": "java.lang.String",
      "description": "The token table name.",
      "defaultValue": "t_token"
    },
    {
      "name": "cn.spark2fire.auth.redis-prefix",
      "type": "java.lang.String",
      "description": "The redis token key prefix.",
      "defaultValue": "TOKEN:a:"
    },
    {
      "name": "cn.spark2fire.auth.jwt.secret-key",
      "type": "java.lang.String",
      "description": "The secret key for JWT token."
    },
    {
      "name": "cn.spark2fire.auth.jwt.issuer",
      "type": "java.lang.String",
      "description": "The issuer for JWT token.",
      "defaultValue": "spark2fire"
    },
    {
      "name": "cn.spark2fire.auth.jwt.subject",
      "type": "java.lang.String",
      "description": "The subject for JWT token.",
      "defaultValue": "auth0"
    },
    {
      "name": "cn.spark2fire.auth.jwt.audience",
      "type": "java.lang.String",
      "description": "The audience for JWT token.",
      "defaultValue": "web"
    }
  ]
}
